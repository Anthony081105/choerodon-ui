"use strict";

var _interopRequireDefault = require("@babel/runtime/helpers/interopRequireDefault");

var _react = _interopRequireDefault(require("react"));

var _enzyme = require("enzyme");

var _PagnationTest = _interopRequireDefault(require("./PagnationTest"));

var setupByMount = function setupByMount() {
  var wrapper = (0, _enzyme.mount)(_react["default"].createElement(_PagnationTest["default"], null));
  return {
    wrapper: wrapper
  };
};

describe('Tree-pro', function () {
  beforeEach(function () {
    jest.useFakeTimers();
  });
  afterEach(function () {
    jest.useRealTimers();
  });
  it('should have a pageSize 20 ', function () {
    var _setupByMount = setupByMount(),
        wrapper = _setupByMount.wrapper;

    expect(wrapper.find('Select').prop('value')).toBe('20');
  });
  it('set showQuickJumper should have a NumberField render ', function () {
    var _setupByMount2 = setupByMount(),
        wrapper = _setupByMount2.wrapper;

    wrapper.setProps({
      showQuickJumper: true
    });
    expect(wrapper.find('NumberField').length).toBe(1);
  });
  it('set disable Select should have a disable props ', function () {
    var _setupByMount3 = setupByMount(),
        wrapper = _setupByMount3.wrapper;

    wrapper.setProps({
      disabled: true
    });
    expect(wrapper.find('Select').prop('disabled')).toBe(true);
  });
  it('set showQuickJumper and click handle ', function () {
    var _setupByMount4 = setupByMount(),
        wrapper = _setupByMount4.wrapper;

    wrapper.find('Pager').at(3).simulate('click');
    expect(wrapper).toMatchSnapshot();
  });
});
