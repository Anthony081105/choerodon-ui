"use strict";

var _interopRequireDefault = require("@babel/runtime/helpers/interopRequireDefault");

var _package = _interopRequireDefault(require("../package.json"));

var testDist = process.env.LIB_DIR === 'dist';
describe('choerodon-ui dist files', function () {
  // https://github.com/ant-design/ant-design/issues/1638
  // https://github.com/ant-design/ant-design/issues/1968
  it('exports modules correctly', function () {
    var choerodon = testDist ? require('../dist/choerodon-ui') : require('../components'); // eslint-disable-line global-require

    expect(Object.keys(choerodon)).toMatchSnapshot();
  });
  it('exports modules component-pro correctly', function () {
    var choerodon = testDist ? require('../dist/choerodon-ui-pro') : require('../components-pro'); // eslint-disable-line global-require

    expect(Object.keys(choerodon)).toMatchSnapshot();
  }); // https://github.com/ant-design/ant-design/issues/1970
  // https://github.com/ant-design/ant-design/issues/1804

  if (testDist) {
    it('should have choerodon-ui.version', function () {
      var choerodon = require('../dist/choerodon-ui'); // eslint-disable-line global-require


      expect(choerodon.version).toBe(_package["default"].version);
    });
  }
});
