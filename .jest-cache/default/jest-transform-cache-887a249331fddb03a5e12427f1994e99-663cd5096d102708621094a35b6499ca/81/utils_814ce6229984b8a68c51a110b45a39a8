"use strict";

Object.defineProperty(exports, "__esModule", {
  value: true
});
exports.selectDate = selectDate;
exports.hasSelected = hasSelected;
exports.openPanel = openPanel;
exports.clearInput = clearInput;
exports.nextYear = nextYear;
exports.nextMonth = nextMonth;

/* eslint-disable import/prefer-default-export */
function selectDate(wrapper, date, index) {
  var calendar = wrapper;

  if (index !== undefined) {
    calendar = wrapper.find('.c7n-calendar-range-part').at(index);
  }

  calendar.find({
    title: date.format('LL'),
    role: 'gridcell'
  }).simulate('click');
}

function hasSelected(wrapper, date) {
  return wrapper.find({
    title: date.format('LL'),
    role: 'gridcell'
  }).hasClass('c7n-calendar-selected-day');
}

function openPanel(wrapper) {
  wrapper.find('.c7n-calendar-picker-input').hostNodes().simulate('click');
}

function clearInput(wrapper) {
  wrapper.find('.c7n-calendar-picker-clear').hostNodes().simulate('click');
}

function nextYear(wrapper) {
  wrapper.find('.c7n-calendar-next-year-btn').simulate('click');
}

function nextMonth(wrapper) {
  wrapper.find('.c7n-calendar-next-month-btn').simulate('click');
}
