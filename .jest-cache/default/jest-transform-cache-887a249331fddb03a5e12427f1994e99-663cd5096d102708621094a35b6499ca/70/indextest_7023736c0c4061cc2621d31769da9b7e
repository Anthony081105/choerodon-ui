"use strict";

var _interopRequireDefault = require("@babel/runtime/helpers/interopRequireDefault");

var _react = _interopRequireDefault(require("react"));

var _enzyme = require("enzyme");

var _ = _interopRequireDefault(require(".."));

var TabPane = _["default"].TabPane;
describe('Tabs', function () {
  describe('editable-card', function () {
    var handleEdit;
    var wrapper;
    beforeEach(function () {
      handleEdit = jest.fn();
      wrapper = (0, _enzyme.mount)(_react["default"].createElement(_["default"], {
        type: "editable-card",
        onEdit: handleEdit
      }, _react["default"].createElement(TabPane, {
        tab: "foo",
        key: "1"
      }, "foo")));
    });
    it('add card', function () {
      wrapper.find('.c7n-tabs-new-tab').hostNodes().simulate('click');
      expect(handleEdit.mock.calls[0][1]).toBe('add');
    });
    it('remove card', function () {
      wrapper.find('.icon-close').simulate('click');
      expect(handleEdit).toHaveBeenCalledWith('1', 'remove');
    });
  });
  describe('tabPosition', function () {
    it('remove card', function () {
      var wrapper = (0, _enzyme.mount)(_react["default"].createElement(_["default"], {
        tabPosition: "left",
        tabBarExtraContent: "xxx"
      }, _react["default"].createElement(TabPane, {
        tab: "foo",
        key: "1"
      }, "foo")));
      expect(wrapper).toMatchSnapshot();
    });
  });
});
