"use strict";

var _interopRequireDefault = require("@babel/runtime/helpers/interopRequireDefault");

var _ = require("..");

var _en_US = _interopRequireDefault(require("../en_US"));

var _LocaleContext = _interopRequireDefault(require("../LocaleContext"));

describe('LocaleContext', function () {
  it('LocaleContext return transfer front', function () {
    var TransferFont = (0, _.$l)('Transfer', 'items');
    expect(TransferFont).toEqual('项');
  });
  it('LocaleContext change lauguage return transfer front', function () {
    _LocaleContext["default"].setLocale(_en_US["default"]);

    var TransferFont = (0, _.$l)('Transfer', 'items');
    expect(TransferFont).toEqual('items');
  });
  it('LocaleContext change supports', function () {
    expect(_LocaleContext["default"].supports).toEqual({
      zh_CN: '简体中文',
      en_GB: 'English',
      en_US: 'English(US)'
    });

    _LocaleContext["default"].setSupports({
      zh_CN: '简体中文',
      en_GB: 'English',
      ja_JP: '日本語'
    });

    expect(_LocaleContext["default"].supports).toEqual({
      zh_CN: '简体中文',
      en_GB: 'English',
      ja_JP: '日本語'
    });
  });
});
