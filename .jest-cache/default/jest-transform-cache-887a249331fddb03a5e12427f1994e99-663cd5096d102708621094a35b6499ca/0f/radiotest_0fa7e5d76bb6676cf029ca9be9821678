"use strict";

var _interopRequireDefault = require("@babel/runtime/helpers/interopRequireDefault");

var _react = _interopRequireDefault(require("react"));

var _enzyme = require("enzyme");

var _radio = _interopRequireDefault(require("../radio"));

var _focusTest = _interopRequireDefault(require("../../../tests/shared/focusTest"));

describe('Radio', function () {
  (0, _focusTest["default"])(_radio["default"]);
  it('should render correctly', function () {
    var wrapper = (0, _enzyme.render)(_react["default"].createElement(_radio["default"], {
      className: "customized"
    }, "Test"));
    expect(wrapper).toMatchSnapshot();
  });
  it('responses hover events', function () {
    var onMouseEnter = jest.fn();
    var onMouseLeave = jest.fn();
    var wrapper = (0, _enzyme.shallow)(_react["default"].createElement(_radio["default"], {
      onMouseEnter: onMouseEnter,
      onMouseLeave: onMouseLeave
    }));
    wrapper.simulate('mouseenter');
    expect(onMouseEnter).toHaveBeenCalled();
    wrapper.simulate('mouseleave');
    expect(onMouseLeave).toHaveBeenCalled();
  });
});
