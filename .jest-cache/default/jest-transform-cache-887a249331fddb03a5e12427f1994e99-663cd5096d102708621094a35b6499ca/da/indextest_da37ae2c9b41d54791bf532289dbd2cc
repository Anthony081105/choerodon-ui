"use strict";

var _interopRequireDefault = require("@babel/runtime/helpers/interopRequireDefault");

var _react = _interopRequireDefault(require("react"));

var _enzyme = require("enzyme");

var _index = _interopRequireDefault(require("../index"));

describe('Badge', function () {
  test('badge dot not scaling count > 9', function () {
    var badge = (0, _enzyme.mount)(_react["default"].createElement(_index["default"], {
      count: 10,
      dot: true
    }));
    expect(badge.find('.c7n-card-multiple-words').length).toBe(0);
  });
  test('badge dot not showing count == 0', function () {
    var badge = (0, _enzyme.mount)(_react["default"].createElement(_index["default"], {
      count: 0,
      dot: true
    }));
    expect(badge.find('.c7n-badge-dot').length).toBe(0);
  });
});
