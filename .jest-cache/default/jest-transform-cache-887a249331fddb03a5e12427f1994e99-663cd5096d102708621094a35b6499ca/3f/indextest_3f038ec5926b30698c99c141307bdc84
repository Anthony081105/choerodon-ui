var _interopRequireDefault = require("@babel/runtime/helpers/interopRequireDefault");

var _react = _interopRequireDefault(require("react"));

var _enzyme = require("enzyme");

var _ = _interopRequireDefault(require(".."));

describe('Slider', function () {
  it('should show tooltip when hovering slider handler', function () {
    var wrapper = (0, _enzyme.mount)(_react["default"].createElement(_["default"], {
      defaultValue: 30
    }));
    wrapper.find('.c7n-slider-handle').at(0).simulate('mouseEnter');
    expect((0, _enzyme.render)(wrapper.find('Trigger').instance().getComponent())).toMatchSnapshot();
    wrapper.find('.c7n-slider-handle').at(0).simulate('mouseLeave');
    expect((0, _enzyme.render)(wrapper.find('Trigger').instance().getComponent())).toMatchSnapshot();
  });
});
