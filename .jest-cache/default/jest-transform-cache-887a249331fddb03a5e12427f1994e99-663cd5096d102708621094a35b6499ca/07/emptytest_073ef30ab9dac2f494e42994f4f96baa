describe('Table', function () {
  it('renders empty table', function () {
    var wrapper = (0, _enzyme.render)(_react["default"].createElement(_["default"], {
      dataSource: [],
      columns: columns,
      pagination: false
    }));
    expect(wrapper).toMatchSnapshot();
  });
  it('renders empty table with fixed columns', function () {
    var wrapper = (0, _enzyme.render)(_react["default"].createElement(_["default"], {
      dataSource: [],
      columns: columnsFixed,
      pagination: false
    }));
    expect(wrapper).toMatchSnapshot();
  });
  it('renders empty table with custom emptyText', function () {
    var wrapper = (0, _enzyme.render)(_react["default"].createElement(_["default"], {
      dataSource: [],
      columns: columns,
      pagination: false,
      locale: {
        emptyText: 'custom empty text '
      }
    }));
    expect(wrapper).toMatchSnapshot();
  });
  it('renders empty table without emptyText when loading', function () {
    var wrapper = (0, _enzyme.render)(_react["default"].createElement(_["default"], {
      dataSource: [],
      columns: columns,
      loading: true
    }));
    expect(wrapper).toMatchSnapshot();
  });
