"use strict";

var OLD_NODE_ENV = process.env.NODE_ENV;
process.env.NODE_ENV = 'development';
var warnSpy = jest.spyOn(console, 'warn').mockImplementation(function () {});

var choerodon = require('..');

describe('choerodon-ui/pro', function () {
  afterAll(function () {
    process.env.NODE_ENV = OLD_NODE_ENV;
  });
  it('exports modules correctly', function () {
    expect(Object.keys(choerodon)).toMatchSnapshot();
  });
  it('should hint when import all components', function () {
    expect(warnSpy).toHaveBeenCalledWith('You are using a whole package of choerodon-ui/pro, please use https://www.npmjs.com/package/babel-plugin-import to reduce app bundle size.');
    warnSpy.mockRestore();
  });
});
