/**
 * 生成Progress percent: 0.1 -> 0.98
 *   - for ie
 */


function genPercentAdd() {
  /* istanbul ignore next */
  cov_1svl28vqav().f[2]++;
  var k =
  /* istanbul ignore next */
  (cov_1svl28vqav().s[9]++, 0.1);
  var i =
  /* istanbul ignore next */
  (cov_1svl28vqav().s[10]++, 0.01);
  var end =
  /* istanbul ignore next */
  (cov_1svl28vqav().s[11]++, 0.98);

  /* istanbul ignore next */
  cov_1svl28vqav().s[12]++;
  return function (s) {
    /* istanbul ignore next */
    cov_1svl28vqav().f[3]++;
    var start =
    /* istanbul ignore next */
    (cov_1svl28vqav().s[13]++, s);

    /* istanbul ignore next */
    cov_1svl28vqav().s[14]++;

    if (start >= end) {
      /* istanbul ignore next */
      cov_1svl28vqav().b[1][0]++;
      cov_1svl28vqav().s[15]++;
      return start;
    } else
    /* istanbul ignore next */
    {
      cov_1svl28vqav().b[1][1]++;
    }

    cov_1svl28vqav().s[16]++;
    start += k;

    /* istanbul ignore next */
    cov_1svl28vqav().s[17]++;
    k -= i;

    /* istanbul ignore next */
    cov_1svl28vqav().s[18]++;

    if (k < 0.001) {
      /* istanbul ignore next */
      cov_1svl28vqav().b[2][0]++;
      cov_1svl28vqav().s[19]++;
      k = 0.001;
    } else
    /* istanbul ignore next */
    {
      cov_1svl28vqav().b[2][1]++;
    }

    cov_1svl28vqav().s[20]++;
    return start * 100;
  };
}

